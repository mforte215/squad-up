    <main>
        <form class="pt-2" id="form">
            <div class="container-fluid row">
                <div class="col-lg-3 col-sm-4 col-6 text-center">
                    <label class= "text-decoration-underline text-black">First Name:</label> <br><br>
                    <label class= "text-decoration-underline text-black">Last Name:</label> <br><br>
                    <label class= "text-decoration-underline text-black" for="phone">Enter a phone number:</label><br>
                    <small class= "fst-italic">Format: 123-459-6278</small> <br><br>
                    <label class= "text-decoration-underline text-black" for="email">Enter your email:</label> <br><br>
                    <label class= "mb-2 text-decoration-underline text-black">Current Address:</label> <br><br>
                    <label class= "mb-4 text-decoration-underline text-black">Personal GitHub Link:</label> <br>
                    <label class= "mb-4 text-decoration-underline text-black">School/University:</label> <br>
                    <label class= "mb-4 text-decoration-underline text-black">Most Recent Job Title:</label> <br> 
                    <label class= "mb-5 text-decoration-underline text-black">Time in role:</label>  <br>
                    <label class= "mb-4 text-decoration-underline text-black">#1 Role Responsibility</label> <br><br> 
                    <label class= "mb-4 text-decoration-underline text-black">#2 Role Responsibility</label> <br><br>
                    <label class= "mb-4 text-decoration-underline text-black">#3 Role Responsibility</label> <br><br>
                    <label class= "mb-4 text-decoration-underline text-black">#4 Role Responsibility</label> <br><br>
                </div>
                <div class="col-lg-8 col-sm-6 col-5">
                    <input required class= "border-black rounded-2" id='firstName' type= "text" placeholder="John"> <br><br>
                    <input required  class= "border-black rounded-2" id="lastName" type="text" placeholder="Doe"> <br> <br>
                    <input required  class= "border-black rounded-2" type="tel" id="phone" name="phone" placeholder="123-452-6728" pattern="[0-9]{3}-[0-9]{3}-[0-9]{4}" > <br><br>
                    <input required  class= "border-black rounded-2" type="email" id="email" name="email" placeholder="myEmail@email.com"> <br><br>
                    <input required  class= "border-black rounded-2" type="text" id="location" placeholder="Philadelphia, PA"> <br><br>
                    <input required  class= "border-black rounded-2" type="url" id="github"> <br><br>
                    <input required  class= "border-black rounded-2" id='schoolOne' type= "text" placeholder="Ex: University of Pennsylvania"> <br><br>
                    <input required  class= "border-black rounded-2" id='jobOne' type= "text" placeholder="Ex: Senior Developer"> <br><br>
                    <input required  class= "border-black rounded-2" id='atJob' type= "text" placeholder="Ex: 2022-Present"> <br><br>
                    <input required  class= "input-group input-group-lg border-black rounded-2" id='respoOne' type= "text"> <br><br>
                    <input required  class= "input-group input-group-lg border-black rounded-2" id='respoTwo' type= "text"> <br><br>
                    <input required  class= "input-group input-group-lg border-black rounded-2" id='respoThree' type= "text"> <br><br>
                    <input required  class= "input-group input-group-lg border-black rounded-2" id='respoFour' type= "text"> <br><br>
                    <button class="btn btn-secondary btn-lg" id="send">Submit</button>
                </div>
            </div>
        </form>
        <div id="pdfButton">
        </div>
        
    </main>
    <script src="./js/resume.js"></script>

